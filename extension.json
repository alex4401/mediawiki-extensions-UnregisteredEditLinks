{
	"name": "Unregistered Edit Links",
	"version": "1.1.0",

	"author": [
		"alex4401"
	],
	"url": "https://github.com/alex4401/mediawiki-extensions-UnregisteredEditLinks",
	"descriptionmsg": "unregisterededitlinks-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",

	"requires": {
		"MediaWiki": ">= 1.37.0",
		"platform": {
			"php": ">= 7.4"
		}
	},

	"MessagesDirs": {
		"AnonEditFlow": [
			"i18n"
		]
	},

	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AnonEditFlow\\": "includes/"
	},
	
	"HookHandlers": {
		"Main": {
			"class": "MediaWiki\\Extension\\AnonEditFlow\\AnonEditFlowHooks"
		}
	},
	"Hooks": {
		"SkinTemplateNavigation::Universal": "Main",
		"LoginFormValidErrorMessages": "Main"
	},

	"config_prefix": "wgAEF",
	"config": {
		"AdvertiseCreationInContentNs": {
			"value": true
		},
		"AdvertiseCreationIfCanExist": {
			"value": false
		}
	},

	"manifest_version": 2
}
